{
  "name": "ngx-connect-four",
  "version": "0.0.0",
  "scripts": {
    "doc.all": "npm run doc.cfb",
    "doc.cfb": "compodoc -p ./projects/connect-four-board/tsconfig.lib.json -d ./docs/ngx-cfb --theme material",
    "build.all": "npm run build.cfb",
    "build.all.prod": "npm run build.cfb.prod",
    "build.cfb": "ng build connect-four-board",
    "build.cfb.prod": "npm run copy-all && npm run ng build connect-four-board --prod",
    "test.all": "npm run test.cfb",
    "test.cfb": "ng test connect-four-board --code-coverage",
    "codecov": "codecov -f coverage/*.json",
    "lint.all": "npm run lint.cfb",
    "lint.cfb": "tslint --project ./projects/connect-four-board/tsconfig.lib.json",
    "copy-all": "npm run copy-readme && npm run copy-license",
    "copy-readme": "cp README.md dist/connect-four-board",
    "copy-license": "cp LICENSE.md dist/connect-four-board"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/animations": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular-devkit/build-ng-packagr": "~0.6.1",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@compodoc/compodoc": "^1.1.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codecov": "^3.0.2",
    "codelyzer": "~4.2.1",
    "convert-md": "^0.1.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "standard-version": "^4.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}